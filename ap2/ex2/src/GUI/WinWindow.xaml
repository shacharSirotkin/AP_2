<Window x:Class="GUI.WinWindow" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="You Won!!" Height="350" Width="300">
    <StackPanel>
        <Canvas Height="300" Width="300" VerticalAlignment="Top" HorizontalAlignment="Center">
            <Canvas.Background>
                <ImageBrush ImageSource="Images/Win.jpg"/>
            </Canvas.Background>
        </Canvas>
        <Button Name="Yay" Content="Yay!!" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,-10,0,0" Click="Yay_Click"/>
    </StackPanel>
</Window>
